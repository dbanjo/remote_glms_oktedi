  COPY_ORIGINAL          CHAR(1 BYTE),
  SAVE_TEMPLATE          CHAR(1 BYTE),
  USE_PRINTING_LABEL     CHAR(1 BYTE),
  PRINTER_LABEL_USE      CHAR(1 BYTE),
  PRINTER_INVOICE_USE    CHAR(1 BYTE),
  USE_PRINTING_INVOICE   CHAR(1 BYTE),
  SOURCE_DIRECTORY       VARCHAR2(4000 BYTE),
  TARGET_DIRECTORY       VARCHAR2(4000 BYTE),
  PRINTING_TEMPLATE      VARCHAR2(1000 BYTE),
  LABEL_PRINTER          VARCHAR2(1000 BYTE),
  LABEL_TRAY             VARCHAR2(1000 BYTE),
  LABEL_TEMPLATE         VARCHAR2(1000 BYTE),
  LABEL_OUTPUTDIR        VARCHAR2(1000 BYTE),
  LABEL_FILENAME_MASK    VARCHAR2(1000 BYTE),
  INVOICE_PRINTER        VARCHAR2(1000 BYTE),
