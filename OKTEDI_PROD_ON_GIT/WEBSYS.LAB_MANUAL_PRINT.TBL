  INVOICE_TEMPLATE       VARCHAR2(1000 BYTE),
  INVOICE_OUTPUTDIR      VARCHAR2(1000 BYTE),
  INVOICE_FILENAME_MASK  VARCHAR2(1000 BYTE),
  SAVE_WEB_QUALITY       CHAR(1 BYTE),
  PREFIX_POID            CHAR(1 BYTE),
  WEBSERVICE_URL         VARCHAR2(1000 BYTE),
  ENCRYPTION_KEY         VARCHAR2(1000 BYTE),
  ONFAILURE              CHAR(1 BYTE),
  RETRY_EVERY            NUMBER(16),
  WEBSERVICE_RESULT_ACT  CHAR(1 BYTE),
  EMAIL_ADDRESS          VARCHAR2(4000 BYTE),
  WEBSERVICE_PK          VARCHAR2(10 BYTE)
)
TABLESPACE PICTION_MED_1
PCTUSED    0
PCTFREE    0
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          32K
            NEXT             512K
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE;
